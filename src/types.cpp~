#include "types.h"

using namespace orelay;

#ifdef RTAI
long long orelay_gettime(void){
//	return rt_get_cpu_time_ns();
	return rt_get_real_time_ns();
	}
#endif
#ifndef RTAI
long long orelay_gettime(void){
	struct timespec x;
	long long r;
	clock_gettime(CLOCK_REALTIME, &x);
	r=(long long)(x.tv_sec*1E9+x.tv_nsec);
	return r;
	}
#endif

